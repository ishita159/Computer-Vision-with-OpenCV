#!/home/ishita/PycharmProjects/openCVExamples/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'Tinkerer==1.7.2','console_scripts','tinker'
__requires__ = 'Tinkerer==1.7.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Tinkerer==1.7.2', 'console_scripts', 'tinker')()
    )
